extends layout

block content

  //- var teams = ["e","e","y"]
  - var teams = ["Sector One", "BDS", "OG Esports", "BeGenius", "Vitality", "Wylde", "Valar Morghulis", "Mandatory"]
  div.header
    h2 VRL team scoreboard
    img.vrl_logo(src="../images/VRL_Logo.png", alt="vrl-logo")
    p See live updated scores for the VRL
  div.teamList
    div.headings
        h3 teams
        h3 maps won
    each n in teams
        div.table
            div.teamCard
                p= n

            div.scores
                p= n